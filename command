[[ $(grep -hso -P '(?<=Host\(`)(.+)(?=`)' $(docker ps --format '{{.Label "com.docker.compose.project.working_dir"}}/{{.Label "com.docker.compose.project.config_files"}}')) != '' ]] && grep -hso -P '(?<=Host\(`)(.+)(?=`)' $(docker ps --format '{{.Label "com.docker.compose.project.working_dir"}}/{{.Label "com.docker.compose.project.config_files"}}') | sed -e 's/`) || Host(`/\n/' | sed -e 's/`)//' | sort -u | sed -e 's/^/https:\/\//' | xargs -n 1 curl -Li -o /dev/null -w '%{http_code}|%{url_effective}\n' -s -k || grep -hso -P '((?<=hostname: )|(?<=domain=))(.+)' $(docker ps --format '{{.Label "com.docker.compose.project.working_dir"}}/{{.Label "com.docker.compose.project.config_files"}}') | sed -e 's/`) || Host(`/\n/' | sed -e 's/`)//' | sort -u | sed -e 's/^/https:\/\//' | xargs -n 1 curl -Li -o /dev/null -w '%{http_code}|%{url_effective}\n' -s -k